      ]
    },
    "intracellular": {
      "enabled": false,
      "execution_order": [],
      "functions": []
    },
    "diffusion": {
      "enabled": false,
      "execution_order": [],
      "functions": []
    },
    "intercellular": {
      "enabled": false,
      "execution_order": [],
      "functions": []
    },
    "finalization": {
      "enabled": true,
      "execution_order": ["print_summary"],
      "functions": [
        {
          "id": "print_summary",
          "function_name": "print_simulation_summary",
          "parameters": {},
          "enabled": true,
          "position": {"x": 100, "y": 500}
        }
      ]
    }
  }
